  "scripts": {
    "test": "jest --watch",
    "debug": "NODE_ENV=debug npm test",
    "build:client:analyze": "NODE_ENV=production webpack --config webpack.client.analyze.js",
    "build:client:dev": "NODE_ENV=development webpack --config webpack.client.development.js --watch",
    "build:client": "NODE_ENV=production webpack --config webpack.client.production.js",
    "build:server:dev": "NODE_ENV=development webpack --watch --config webpack.server.development.js",
    "build:server": "NODE_ENV=production webpack --config webpack.server.production.js",
    "start:dev": "rm -rf public dist && concurrently \"npm run build:client:dev\" \"sh waitOn.sh public/manifest.json && npm run build:server:dev\" \"sh waitOn.sh dist/server.js && nodemon --legacy-watch\"",
    "start": "node dist/server.js",
    "build": "npm run build:client && npm run build:server",
    "lint": "tslint --project . --fix",
    "stylelint": "stylelint --fix src/**/*.css"
  },
